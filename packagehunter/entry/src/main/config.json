{
  "app": {
    "bundleName": "github.nisrulz.projectpackagehunter",
    "vendor": "nisrulz",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "reqPermissions": [
      {
        "usedScene": {
          "ability": [
            "github.nisrulz.projectpackagehunter.slice.MainAbilitySlice"
          ],
          "when": "always"
        },
        "reason": "necessary",
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED"
      }
    ],
    "package": "github.nisrulz.projectpackagehunter",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "github.nisrulz.projectpackagehunter.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}